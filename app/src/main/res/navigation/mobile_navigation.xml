<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation.xml"
    app:startDestination="@id/navigation_event">

    <fragment
        android:id="@+id/navigation_event"
        android:name="com.example.joingreen.Ui.Event.EventFragment"
        android:label="Event"
        tools:layout = "@layout/event"/>
    <fragment
        android:id="@+id/navigation_reward"
        android:name="com.example.joingreen.Ui.Reward.RewardFragment"
        android:label="Reward"
        tools:layout="@layout/reward"/>
    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.joingreen.Ui.User.UserFragment"
        android:label="User"
        tools:layout="@layout/login"/>
</navigation>